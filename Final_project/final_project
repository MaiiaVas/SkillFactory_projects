    "#MONTH transactions\n",
    "cols = ['month', 'event']\n",
    "month_ev_all = events[cols]\n",
    "month_ev_all['count'] = 1\n",
    "\n",
    "month_ev_vis_all = month_ev_all.groupby(['month','event']).sum()\n",
    "month_ev_vis_all = month_ev_vis_all.reset_index()\n",
    "\n",
    "#DAY transactions\n",
    "cols = ['day_week', 'event']\n",
